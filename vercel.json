{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      },
      "api/src/css/*.php": {
        "runtime": "vercel-php@0.6.0"
      },
      "api/src/js/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
        { "src": "/css/(.*)", "dest": "/src/css/$1" },
        { "src": "/js/(.*)", "dest": "/src/js/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ]
  }