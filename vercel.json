{
    "functions": {
      "api/**/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      { "src": "/(.*)",  "dest": "/api/index.php" },
      { "src": "/css/style.css", "dest": "/css/style.css" },
      { "src": "/js/profile.js", "dest": "/js/profile.js" },
      { "src": "/js/control.js", "dest": "/js/control.js" }]
  }